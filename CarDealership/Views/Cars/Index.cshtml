@using CarDealership.Models;

<h1>Plateau Group Car Dealership</h1>

@if (Model.Count == 0)
{
  <p>There are no cars in the list.</p>
}
<ul>
  @foreach (Car item in Model)
  {
    <li>@item.MakeModel</li>
    <li>@item.Price</li>
    <li>@item.Miles</li>
  }
</ul>
<a href="/cars/new">Add a new car.</a>